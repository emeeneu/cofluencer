<h1>Profile</h1>

<a href="/logout">logout</a>

<h2>Search user</h2>
<form method="POST" action="profile">
  IG User:
  <input type="text" id="name" name="name"></input>
  <button type="submit" class="btn btn-indigo">
    <i class="fa fa-bolt" aria-hidden="true"></i>Search IG user</button>
</form>

<% if( typeof(iguser) != 'undefined') { %>
  <h1>IG user:
    <%= iguser.username %>
  </h1>
  <h2>followers:
    <%= iguser.followers_count%>
  </h2>
  <% iguser.media.data.forEach((image)=>{ %>
    <div style="float: left">
      <img src='<%=image.media_url%>' style="width: 300px; height: 300px">
      <br> ðŸ’š
      <%= image.like_count%>
        <br>
    </div>
    <% }); %>
<% } %>
